<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:javaee="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID">
  <javaee:display-name>PdksWebService</javaee:display-name>
  <welcome-file-list>
    <welcome-file>version.jsp</welcome-file>
  </welcome-file-list>
  <javaee:resource-ref>
    <javaee:res-ref-name>jdbc/pdks</javaee:res-ref-name>
    <javaee:res-type>javax.sql.DataSource</javaee:res-type>
    <javaee:res-auth>Container</javaee:res-auth>
  </javaee:resource-ref>
  <servlet>
    <javaee:display-name>cxf</javaee:display-name>
    <servlet-name>cxf</servlet-name>
    <servlet-class>org.apache.cxf.transport.servlet.CXFServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <javaee:display-name>Startup</javaee:display-name>
    <servlet-name>Startup</servlet-name>
    <servlet-class>org.pdks.system.action.Startup</servlet-class>
    <init-param>
      <javaee:param-name>webDS</javaee:param-name>
      <javaee:param-value>java:comp/env/jdbc/pdks</javaee:param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
    <javaee:description>JAX-RS Tools Generated - Do not modify</javaee:description>
    <servlet-name>JAX-RS Servlet</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
    <init-param>
      <javaee:param-name>com.sun.jersey.config.property.packages</javaee:param-name>
      <javaee:param-value>com.pdks.webService</javaee:param-value>
    </init-param>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>JAX-RS Servlet</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>cxf</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <context-param>
    <javaee:param-name>contextConfigLocation</javaee:param-name>
    <javaee:param-value>WEB-INF/beans.xml,/WEB-INF/applicationContext*.xml</javaee:param-value>
  </context-param>
  <listener>
    <javaee:listener-class>org.springframework.web.context.ContextLoaderListener</javaee:listener-class>
  </listener>
</web-app>