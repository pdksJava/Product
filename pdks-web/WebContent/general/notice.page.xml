<?xml version="1.0" encoding="UTF-8"?>
<page view-id="#{menuItemConstant.notice}" xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd">
		<restrict>#{userHome.hasPermission('notice','view')}</restrict>
	<rewrite pattern="/notice"/>
	<action execute="#{noticeHome.fillNotice}" on-postback="false"/>
	<navigation>
            <rule if-outcome="notice">
                <redirect view-id="#{menuItemConstant.notice}"/>
            </rule>
        </navigation>
</page>
